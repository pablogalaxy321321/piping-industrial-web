{
  "name": "piping-web",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "frames:1440": "node ./scripts/extract-frames.mjs",
    "frames:1280": "cross-env WIDTH=1280 node ./scripts/extract-frames.mjs",
    "frames:1920": "cross-env WIDTH=1920 node ./scripts/extract-frames.mjs",
    "frames:640": "cross-env WIDTH=640 node ./scripts/extract-frames.mjs",
    "frames:custom": "cross-env INPUT=./public/videos/maqueta_virtual_piping_cabecera.mp4 OUT_DIR=./public/frames/%WIDTH% FPS=%FPS% WIDTH=%WIDTH% QUALITY=%QUALITY% node ./scripts/extract-frames.mjs",
    "frames:all": "npm run frames:640 && npm run frames:1280 && npm run frames:1440 && npm run frames:1920"
  },
  "dependencies": {
    "gsap": "^3.12.5",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-router-dom": "^6.28.0",
    "three": "^0.169.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.36.0",
    "@types/react": "^19.1.16",
    "@types/react-dom": "^19.1.9",
    "@vitejs/plugin-react": "^5.0.4",
    "autoprefixer": "^10.4.20",
    "cross-env": "^7.0.3",
    "eslint": "^9.36.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.22",
    "ffmpeg-static": "^5.2.0",
    "globals": "^16.4.0",
    "postcss": "^8.4.49",
    "tailwindcss": "^3.4.15",
    "vite": "^7.1.7"
  }
}
